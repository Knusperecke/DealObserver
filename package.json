{
  "name": "canyon-grabber",
  "version": "1.0.0",
  "description": "Catch deals and price changes",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha \"test/**/*.mocha.js\"",
    "test-browser": "karma start",
    "check-format": "check-clang-format \"npm run format\"",
    "format": "prettier --write ./",
    "lint": "eslint ./ --cache --ignore-pattern .gitignore",
    "precommit-msg": "echo 'Pre-commit checks ...' && exit 0",
    "grabber": "node src/grabber/main.js",
    "grabber-initial": "node src/grabber/main.js --summary",
    "prepare": "husky install"
  },
  "pre-commit": [
    "precommit-msg",
    "lint",
    "check-format"
  ],
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "eslint": "^4.19.1",
    "eslint-plugin-mocha": "^4.12.1",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^2.0.13",
    "karma-webpack-preprocessor": "^1.0.1",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "mysql2": "^3.6.0",
    "npm": "^5.10.0",
    "nyc": "^11.9.0",
    "pre-commit": "^1.2.2",
    "sinon": "^4.5.0",
    "webpack": "^3.12.0",
    "xmlhttprequest": "^1.8.0"
  },
  "author": "Tobias Hilbrich",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
